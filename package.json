{
  "name": "cloudstorm",
  "version": "0.6.2",
  "description": "Minimalistic Discord Gateway library",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build:src": "tsup src/index.ts --dts --sourcemap --format cjs --target node12 --minify",
