  "devDependencies": {
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "tsup": "^6.5.0",
    "typedoc": "^0.23.24",
    "typedoc-plugin-mdn-links": "^2.0.2",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
