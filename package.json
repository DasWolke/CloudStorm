{
  "name": "cloudstorm",
  "version": "0.15.1",
  "description": "Minimalistic Discord Gateway library",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build:src": "tsup src/index.ts --clean --dts --sourcemap --format cjs --target node22 --minify-whitespace --minify-syntax --treeshake && node ./sourceMapPostProcess.js",
    "build:docs": "typedoc --name CloudStorm --excludeExternals --sort static-first --sort alphabetical"
  },
  "author": "wolke <wolke@weeb.sh>",
  "license": "MIT",
  "dependencies": {
    "discord-api-types": "^0.38.37",
    "snowtransfer": "^0.16.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.8",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "tsup": "^8.5.1",
    "typedoc": "^0.28.16",
    "typedoc-plugin-mdn-links": "^5.0.10",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ]
}
