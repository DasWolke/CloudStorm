{
  "name": "cloudstorm",
  "version": "0.14.1",
  "description": "Minimalistic Discord Gateway library",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build:src": "tsup src/index.ts --clean --dts --sourcemap --format cjs --target node22 --minify-whitespace --minify-syntax --treeshake && node ./sourceMapPostProcess.js",
    "build:docs": "typedoc --name CloudStorm --excludeExternals --sort static-first --sort alphabetical"
  },
  "author": "wolke <wolke@weeb.sh>",
  "license": "MIT",
  "dependencies": {
    "discord-api-types": "^0.38.21",
    "snowtransfer": "^0.15.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.11",
    "typedoc-plugin-mdn-links": "^5.0.9",
    "typescript": "^5.9.2"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ]
}
