  },
  "author": "wolke <wolke@weeb.sh>",
  "license": "MIT",
  "dependencies": {
    "snowtransfer": "^0.8.3"
  },
  "devDependencies": {
    "@types/node": "20.3.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "tsup": "^7.1.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-mdn-links": "^3.0.3",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ]
}
