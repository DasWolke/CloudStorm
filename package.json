{
  "name": "cloudstorm",
  "version": "0.8.3",
  "description": "Minimalistic Discord Gateway library",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=12.0.0"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build:src": "tsup src/index.ts --clean --dts --sourcemap --format cjs --target node12 --minify-whitespace",
